{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "@axios": ["./src/custom/utils/apis/axios.ts"],
      "@actions": ["./src/custom/utils/actions"],
      "@apitypes": ["./src/custom/utils/types"],
      "@transformers": ["./src/custom/utils/transformers"],
      "@apiEndpoints": ["./src/custom/utils/apis/apiEndpoints"],
      "@styles": ["./src/public/styles"],
      "@ui-layouts": ["./src/ui-layouts"],
      "@components": ["./src/components"],
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "html/types/**/*.ts",
    "src/store/common-context.js"
  ],
  "exclude": ["node_modules"]
}
